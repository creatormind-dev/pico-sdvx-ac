{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"args": [
				"--release"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"isDefault": true,
				"kind": "build"
			},
			"label": "rust: cargo build"
		},
		{
			"type": "cargo",
			"command": "run",
			"args": [
				"--release"
			],
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo run",
		}
	]
}